using System;

namespace svoi1
{
    class MainClass
    {
        public class Tree
        {
            public Tree(string name, int n)
            {
                Console.WriteLine(name + " " + n);
            }
        }

        class Prorgram
        {
            public static void Main()
            {
                var tree1 = new Tree("сосна", -6);
                var tree2 = new Tree("береза", 80);
            }
        }
    }
}
