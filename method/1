using System;

namespace met1
{
	class MainClass
	{
		public static void Main ()
		{

			Console.WriteLine("Введите ваш текст: ");

			string str = Console.ReadLine();

			int j = 0;
			foreach(char c in str)
				if (c == 'e')
					j++;

			Console.WriteLine("Найдено латинских |Е|: " + j);
		}
	}
}
